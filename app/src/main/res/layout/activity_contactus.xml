<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorLightGray"
    tools:context=".Contactus">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="21dp"
        android:text="@string/youth_empowerment_foundation1"
        android:textColor="@color/btn_login"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="38dp" />


    <Button
        android:id="@+id/gmail"
        android:layout_width="102dp"
        android:layout_height="96dp"
        android:layout_alignStart="@+id/textView3"
        android:layout_alignTop="@+id/fb"
        android:background="@mipmap/action_g_plus"
        android:padding="15dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="93dp"
        android:layout_alignLeft="@+id/textView3" />

    <Button
        android:id="@+id/fb"
        android:layout_width="102dp"
        android:layout_height="107dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/call"
        android:layout_marginTop="84dp"
        android:background="@mipmap/action_fb"
        android:padding="15dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="93dp"
        android:layout_alignLeft="@+id/call" />

    <Button
        android:id="@+id/insta"
        android:layout_width="92dp"
        android:layout_height="88dp"
        android:layout_alignEnd="@+id/gmail"
        android:layout_alignTop="@+id/call"
        android:background="@mipmap/action_insta"
        android:padding="15dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="217dp"
        android:layout_alignRight="@+id/gmail" />

    <Button
        android:id="@+id/call"
        android:layout_width="102dp"
        android:layout_height="81dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/signout"
        android:layout_marginBottom="203dp"
        android:background="@drawable/call"
        android:padding="15dp"
        tools:ignore="MissingConstraints,NotSibling"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="230dp"
        android:layout_alignLeft="@+id/signout" />


    <Button
        android:id="@+id/about"
        android:layout_width="102dp"
        android:layout_height="93dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="60dp"
        android:layout_marginStart="43dp"
        android:background="@drawable/about"
        android:padding="15dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="349dp"
        android:layout_marginLeft="43dp" />

    <Button
        android:id="@+id/signout"
        android:layout_width="102dp"
        android:layout_height="98dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="57dp"
        android:layout_marginEnd="47dp"
        android:background="@drawable/signout1"
        android:padding="15dp"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="209dp"
        tools:layout_editor_absoluteY="349dp"
        android:layout_alignParentRight="true"
        android:layout_marginRight="47dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/gmail"
        android:layout_marginStart="50dp"
        android:text="Mail Us"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="24sp"
        android:layout_alignParentLeft="true" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/about"
        android:layout_alignStart="@+id/textView3"
        android:text="Instagram"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/gmail"
        android:layout_below="@+id/about"
        android:layout_marginTop="-60dp"
        android:text="AboutUs"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/fb"
        android:layout_below="@+id/gmail"
        android:text="Facebook"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/call"
        android:layout_marginEnd="65dp"
        android:layout_marginTop="-194dp"
        android:text="CallUs"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView8"
        android:layout_below="@+id/about"
        android:layout_marginTop="-60dp"
        android:text="Signout"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="24sp" />


</RelativeLayout>